<div class="session">
  <div class="row">
    <div class="col s12 m12">
<%= form_for(@session) do |f| %>
  <% if @session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@session.errors.count, "error") %> prohibited this session from being saved:</h2>

      <ul>
      <% @session.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
</div>
<div class="col s12 m12">
  <ul>
    <% @users.each do |user| %>
        <li>
          <% #= check_box_tag 'user_ids[]', user.id %>
          <%= check_box_tag "user_ids[#{user.id}]", "#{user.id}", user.id, :name => "user_ids[]" %>
          <%= label_tag "user_ids_#{user.id}", user.first_name + " "+ user.last_name %>
        </li>
    <% end %>
  </ul>
</div >

<div class="section">

</div>
<div class="col s12 m12">
   <a class="actions waves-effect waves-light btn">
     <%= f.submit %>
   </a>
</div>
<% end %>
</div>
</div>
