<p id="notice"><%= notice %></p>

<h1>Listing Sessions</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
    <%= link_to session.created_at, session %>
  </tbody>
  <% end %>
</table>

<br>

<%= link_to 'New Session', new_session_path %>
