<p id="notice"><%= notice %></p>
<%=@session.id %>
<h2>Session created <%= time_ago_in_words @session.created_at %> ago </h2>

    <% @session_users.each do |user| %>
    <ul><%= user.first_name %>
      <table>
        <thead>
          <tr>
            <th>THIS IS THE WIP</th>
            <th></th>
            <th>BLOCKER</th>
          </tr>
        </thead>
        <tbody>
          <tr>

            <td><% @session_wips.where(user_id: user.id, session_id: @session.id).each do |wip| %>
            <ul>
              <li><%= render partial:'wips/form', locals: {wip: wip} %></li>
              <!-- <li><%#= link_to wip.wip_item, wip_path(wip) %></li> -->
            </ul>
            <% end %></td>

            <td><% @session_completeds.where(user_id: user.id, session_id: @session.id).each do |completed| %>
            <ul>
              <li><%= render partial:'completeds/form', locals: {completed: completed} %></li>
              <!-- <li><%#= link_to completed.completed, completed_path(completed) %></li> -->
            </ul>
            <% end %></td>

            <td><% @session_blockers.where(user_id: user.id, session_id: @session.id).each do |blocker| %>
            <ul>
              <li><%= render partial:'blockers/form', locals: {blocker: blocker} %></li>
              <%#= f.submit "clear", :type => "reset" %>
              <%#= link_to 'Destroy', blocker, method: :destroy, data: { confirm: 'Are you sure?' } %>
              <!-- <li><%#= link_to blocker.blocker, blocker_path(blocker) %></li> -->
            </ul>
            <% end %></td>

          </tr>
        <tbody>
      </table>
    </ul>
    <% end %>


<%= link_to 'Edit', edit_session_path(@session) %>
<%= link_to 'Back', sessions_path %>
