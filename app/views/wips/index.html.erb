<p id="notice"><%= notice %></p>

<h1>Listing Wips</h1>

<table>
  <thead>
    <tr>
      <th>Session</th>
      <th>User</th>
      <th>Wip item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wips.each do |wip| %>
      <tr>
        <td><%= wip.session_id %></td>
        <td><%= wip.user_id %></td>
        <td><%= wip.wip_item %></td>
        <td><%= link_to 'Show', wip %></td>
        <td><%= link_to 'Edit', edit_wip_path(wip) %></td>
        <td><%= link_to 'Destroy', wip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wip', new_wip_path %>
